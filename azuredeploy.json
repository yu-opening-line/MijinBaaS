{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "containerSasToken": {
       "type": "string"
    }
  },
  "resources": [
    {
      "name": "netdeploy",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2017-05-10",
      "dependsOn": [ ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://hitachimijin3diag972.blob.core.windows.net/scripts/vmdeploy.json",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "location": {
            "value": "East US"
          },
          "mijin_vm_prefix": {
              "value": "t"
          },
          "mijin_vm_number": {
              "value": "1"
          }
        }
      }
    }
  ],
  "outputs": {
    "result": {
      "type": "string",
      "value": "[reference('netdeploy').outputs.result.value]"
    }
  }
}
