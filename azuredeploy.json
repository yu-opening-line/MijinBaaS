{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "mijin_vm_prefix": {
        "defaultValue": "",
        "type": "String"
    },
    "add_itional_local_IPs": {
        "defaultValue": "",
        "type": "String"
    }
  },
  "variables": {
  },
  "resources": [
    {
      "apiVersion": "2016-07-01",
      "name": "mijin_vm_deploy",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "incremental",
        "templateLink": {
          "uri": "https://github.com/yu000/MijinBaaS/blob/master/vmdeploy.json",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "mijin_vm_prefix": {
              "value": "[parameters('mijin_vm_prefix')]"
          },
          "add_itional_local_IPs": {
              "value": "[parameters('add_itional_local_IPs')]"
          }
        }
      }
    }
  ]
}
