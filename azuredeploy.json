{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "mijin_vm_prefix": {
          "defaultValue": "test",
          "type": "String"
      },
      "mijin_vm_number": {
          "defaultValue": "1",
          "type": "String"
      },
      "add_itional_local_IPs": {
          "defaultValue": "192.168.52.55",
          "type": "String"
      }
    },
    "variables": {
      "virtualMachines_name": "[concat(parameters('mijin_vm_prefix'),'-mijin-vm', parameters('mijin_vm_number'))]",
      "networkInterfaces_name": "[concat(variables('virtualMachines_name'), '-netifs')]",
      "networkSecurityGroups_nsg_name": "[concat(variables('virtualMachines_name'), '-nsg')]",
      "publicIPAddresses_ip_name": "[concat(variables('virtualMachines_name'), '-ip')]",
      "virtualNetworks_vnet_name": "[concat(variables('virtualMachines_name'), '-vnet')]",
      "storageAccounts_diag_name": "[concat(parameters('mijin_vm_prefix'),'mijinvm', parameters('mijin_vm_number'), 'diag')]",
      "storageAccounts_disks_name": "[concat(parameters('mijin_vm_prefix'),'mijinvm', parameters('mijin_vm_number'), 'disks')]"
    },
    "resources": [
        {
            "comments": "リソース '/subscriptions/30692d7a-6cd7-41b1-a712-0b9bdfb1a534/resourceGroups/hitachi-mijin6/providers/Microsoft.Compute/virtualMachines/hitachi-mijin-vm5' ",
            "type": "Microsoft.Compute/virtualMachines",
            "name": "[variables('virtualMachines_name')]",
            "apiVersion": "2016-04-30-preview",
            "location": "centralus",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_A3"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "OpenLogic",
                        "offer": "CentOS",
                        "sku": "7.2",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Linux",
                        "name": "[variables('virtualMachines_name')]",
                        "createOption": "FromImage",
                        "vhd": {
                            "uri": "[concat('https', '://', variables('storageAccounts_disks_name'), '0', '.blob.core.windows.net', concat('/vhds/', variables('virtualMachines_name'),'20170310154041.vhd'))]"
                        },
                        "caching": "ReadWrite"
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[variables('virtualMachines_name')]",
                    "adminUsername": "mijin",
                    "linuxConfiguration": {
                        "disablePasswordAuthentication": true,
                        "ssh": {
                            "publicKeys": [
                                {
                                    "path": "/home/mijin/.ssh/authorized_keys",
                                    "keyData": "ssh-rsa "
                                }
                            ]
                        }
                    },
                    "secrets": []
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces', variables('networkInterfaces_name'))]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true,
                        "storageUri": "[concat('https', '://', variables('storageAccounts_diag_name'), '.blob.core.windows.net', '/')]"
                    }
                }
            },
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', variables('storageAccounts_disks_name'), '0')]",
                "[resourceId('Microsoft.Network/networkInterfaces', variables('networkInterfaces_name'))]",
                "[resourceId('Microsoft.Storage/storageAccounts', variables('storageAccounts_diag_name'))]"
            ]
        },
        {
            "comments": "リソース '/subscriptions/30692d7a-6cd7-41b1-a712-0b9bdfb1a534/resourceGroups/hitachi-mijin6/providers/Microsoft.Network/networkInterfaces/hitachi-mijin-vm5169' 㝋ら一般化㝕れ㝾㝗㝟。",
            "type": "Microsoft.Network/networkInterfaces",
            "name": "[variables('networkInterfaces_name')]",
            "apiVersion": "2016-03-30",
            "location": "centralus",
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipconfig1",
                        "properties": {
                            "privateIPAddress": "10.9.3.4",
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses', variables('publicIPAddresses_ip_name'))]"
                            },
                            "subnet": {
                                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', variables('virtualNetworks_vnet_name')), '/subnets/default')]"
                            }
                        }
                    }
                ],
                "dnsSettings": {
                    "dnsServers": []
                },
                "enableIPForwarding": false,
                "networkSecurityGroup": {
                    "id": "[resourceId('Microsoft.Network/networkSecurityGroups', variables('networkSecurityGroups_nsg_name'))]"
                }
            },
            "dependsOn": [
                "[resourceId('Microsoft.Network/publicIPAddresses', variables('publicIPAddresses_ip_name'))]",
                "[resourceId('Microsoft.Network/virtualNetworks', variables('virtualNetworks_vnet_name'))]",
                "[resourceId('Microsoft.Network/networkSecurityGroups', variables('networkSecurityGroups_nsg_name'))]"
            ]
        },
        {
            "comments": "リソース '/subscriptions/30692d7a-6cd7-41b1-a712-0b9bdfb1a534/resourceGroups/hitachi-mijin6/providers/Microsoft.Network/networkSecurityGroups/hitachi-mijin-vm5-nsg' 㝋ら一般化㝕れ㝾㝗㝟。",
            "type": "Microsoft.Network/networkSecurityGroups",
            "name": "[variables('networkSecurityGroups_nsg_name')]",
            "apiVersion": "2016-03-30",
            "location": "centralus",
            "properties": {
                "securityRules": [
                    {
                        "name": "default-allow-ssh",
                        "properties": {
                            "protocol": "TCP",
                            "sourcePortRange": "*",
                            "destinationPortRange": "22",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 1000,
                            "direction": "Inbound"
                        }
                    },
                    {
                        "name": "mijin5",
                        "properties": {
                            "protocol": "TCP",
                            "sourcePortRange": "*",
                            "destinationPortRange": "7778",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 1010,
                            "direction": "Inbound"
                        }
                    },
                    {
                        "name": "mijin6",
                        "properties": {
                            "protocol": "TCP",
                            "sourcePortRange": "*",
                            "destinationPortRange": "7895",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 1020,
                            "direction": "Inbound"
                        }
                    }
                ]
            },
            "dependsOn": []
        },
        {
            "comments": "リソース '/subscriptions/30692d7a-6cd7-41b1-a712-0b9bdfb1a534/resourceGroups/hitachi-mijin6/providers/Microsoft.Network/publicIPAddresses/hitachi-mijin-vm5-ip' 㝋ら一般化㝕れ㝾㝗㝟。",
            "type": "Microsoft.Network/publicIPAddresses",
            "name": "[variables('publicIPAddresses_ip_name')]",
            "apiVersion": "2016-03-30",
            "location": "centralus",
            "properties": {
                "publicIPAllocationMethod": "Static",
                "idleTimeoutInMinutes": 4
            },
            "dependsOn": []
        },
        {
            "comments": "リソース '/subscriptions/30692d7a-6cd7-41b1-a712-0b9bdfb1a534/resourceGroups/hitachi-mijin6/providers/Microsoft.Network/virtualNetworks/hitachi-mijin5-vnet' 㝋ら一般化㝕れ㝾㝗㝟。",
            "type": "Microsoft.Network/virtualNetworks",
            "name": "[variables('virtualNetworks_vnet_name')]",
            "apiVersion": "2016-03-30",
            "location": "centralus",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.9.3.0/24"
                    ]
                },
                "subnets": [
                    {
                        "name": "default",
                        "properties": {
                            "addressPrefix": "10.9.3.0/24"
                        }
                    }
                ]
            },
            "dependsOn": []
        },
        {
            "comments": "リソース '/subscriptions/30692d7a-6cd7-41b1-a712-0b9bdfb1a534/resourceGroups/hitachi-mijin6/providers/Microsoft.Storage/storageAccounts/hitachimijin5diag972' 㝋ら一般化㝕れ㝾㝗㝟。",
            "type": "Microsoft.Storage/storageAccounts",
            "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
            },
            "kind": "Storage",
            "name": "[variables('storageAccounts_diag_name')]",
            "apiVersion": "2016-01-01",
            "location": "centralus",
            "tags": {},
            "properties": {},
            "dependsOn": []
        },
        {
            "comments": "リソース '/subscriptions/30692d7a-6cd7-41b1-a712-0b9bdfb1a534/resourceGroups/hitachi-mijin6/providers/Microsoft.Storage/storageAccounts/hitachimijin5disks282' 㝋ら一般化㝕れ㝾㝗㝟。",
            "type": "Microsoft.Storage/storageAccounts",
            "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
            },
            "kind": "Storage",
            "name": "[variables('storageAccounts_disks_name')]",
            "apiVersion": "2016-01-01",
            "location": "centralus",
            "tags": {},
            "properties": {},
            "dependsOn": []
        },
        {
            "comments": "リソース '/subscriptions//resourceGroups/cent-rg/providers/Microsoft.Compute/virtualMachines/centostest01/extensions/CustomScriptForLinux' 㝋ら一般化㝕れ㝾㝗㝟。",
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "name": "[concat(variables('virtualMachines_name'),'/extension')]",
            "apiVersion": "2016-03-30",
            "location": "Central US",
            "properties": {
                "publisher": "Microsoft.OSTCExtensions",
                "type": "CustomScriptForLinux",
                "typeHandlerVersion": "1.0",
                "settings": {
                    "fileUris": [
                        "https://hitachimijin3diag972.blob.core.windows.net/scripts/install_mijin.sh"
                    ],
                    "commandToExecute": "[concat('sh install_mijin.sh')]"
                }
            },
            "resources": [],
            "dependsOn": [
                "[resourceId('Microsoft.Compute/virtualMachines', variables('virtualMachines_name'))]"
            ]
        }
    ]
}
